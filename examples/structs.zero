// 结构体示例文件

// 定义一个简单的Person结构体
struct Person {
    name: String,
    age: Integer,
    active: Boolean
};

// 定义一个Point结构体
struct Point {
    x: Integer,
    y: Integer
};

// 类型别名
type Coordinate = Point;

// 创建结构体实例
let person = Person {
    name: "Alice",
    age: 30,
    active: true
};

let point = Point {
    x: 10,
    y: 20
};

// 访问字段
print(person.name);
print(person.age);
print(point.x);
print(point.y);

// 字段赋值
person.age = 31;
print(person.age);