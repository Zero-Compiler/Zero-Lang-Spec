// 重命名导入测试

mod math {
    pub fn add(a: int, b: int) -> int {
        return a + b;
    }

    pub fn multiply(a: int, b: int) -> int {
        return a * b;
    }
}

// 重命名导入
use math::add as plus;
use math::multiply as times;

// 使用重命名后的函数
let result1: int = plus(15, 25);
print(result1);  // 40

let result2: int = times(7, 8);
print(result2);  // 56
