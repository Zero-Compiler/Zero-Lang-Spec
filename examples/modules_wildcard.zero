// 通配符导入测试
// 测试：use module::*

mod utils {
    pub fn double(x: int) -> int {
        return x * 2;
    }

    pub fn triple(x: int) -> int {
        return x * 3;
    }

    pub fn square(x: int) -> int {
        return x * x;
    }
}

use utils::*;

fn main() {
    print(double(5));    // 10
    print(triple(5));    // 15
    print(square(5));    // 25
}
