// multifile_basic.zero - 测试基本的多文件编译

// 从文件加载 math 模块
mod math;

// 使用 use 导入
use math::add;
use math::multiply;

// 测试使用导入的函数
let r1: int = add(10, 20);
print(r1);  // 30

let r2: int = multiply(5, 6);
print(r2);  // 30

// 测试使用路径访问
let r3: int = math::subtract(50, 15);
print(r3);  // 35

let r4: int = math::divide(100, 4);
print(r4);  // 25
