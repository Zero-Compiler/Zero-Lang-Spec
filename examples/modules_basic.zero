// 基础模块系统测试
// 测试：模块声明、公共函数、use 导入

mod math {
    pub fn add(a: int, b: int) -> int {
        return a + b;
    }

    pub fn subtract(a: int, b: int) -> int {
        return a - b;
    }

    fn private_helper() -> int {
        return 42;
    }
}

use math::add;

fn main() {
    let result: int = add(10, 20);
    print(result);
}
