// 符号解析测试
// 测试模块符号注册和导入

mod math {
    pub fn add(a: int, b: int) -> int {
        return a + b;
    }

    pub fn multiply(a: int, b: int) -> int {
        return a * b;
    }

    fn private_helper() -> int {
        return 42;
    }
}

// 单项导入测试
use math::add;

// 使用导入的符号
let result1: int = add(10, 20);
print(result1);  // 应该输出 30

// 多项导入测试
use math::{multiply};

let result2: int = multiply(5, 6);
print(result2);  // 应该输出 30
