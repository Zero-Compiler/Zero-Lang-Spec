// 模块语法最终测试

// 测试pub函数
pub fn add(a: int, b: int) -> int {
    return a + b;
}

// 测试私有函数（默认）
fn subtract(a: int, b: int) -> int {
    return a - b;
}

// 测试模块
mod math {
    fn double(x: int) -> int {
        return x * 2;
    }
}

// 测试use语句（会被忽略，因为还没实现符号解析）
use math::double;

// 直接执行代码
let x: int = add(10, 5);
print(x);  // 应该输出 15

let y: int = subtract(20, 8);
print(y);  // 应该输出 12
